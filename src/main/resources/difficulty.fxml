<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.supsi.minesweeper.view.DifficultyView">
    <VBox fx:id="vbox" alignment="CENTER" spacing="10">
        <Button fx:id="easyButton" text="Easy" style="-fx-font-size: 18;" prefWidth="200" />
        <Button fx:id="mediumButton" text="Medium" style="-fx-font-size: 18;" prefWidth="200" />
        <Button fx:id="hardButton" text="Hard" style="-fx-font-size: 18;" prefWidth="200"/>
    </VBox>
</StackPane>
